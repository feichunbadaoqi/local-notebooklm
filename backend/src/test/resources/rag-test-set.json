{
  "description": "RAG evaluation test set - tracks retrieval quality metrics",
  "testCases": [
    {
      "id": "ml-001",
      "description": "Basic question about machine learning definition",
      "query": "What is machine learning?",
      "expectedChunks": [
        {
          "contentMustContain": ["machine learning", "algorithm", "data"],
          "minRelevanceRank": 3
        }
      ],
      "expectedAnswer": {
        "mustContain": ["machine learning", "algorithm"],
        "mustNotContain": ["quantum computing", "blockchain"]
      },
      "difficulty": "easy"
    },
    {
