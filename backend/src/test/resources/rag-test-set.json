      "query": "What are the differences between supervised and unsupervised learning?",
      "expectedChunks": [
        {
          "contentMustContain": ["supervised learning", "labeled data"],
          "minRelevanceRank": 5
        },
        {
          "contentMustContain": ["unsupervised learning", "unlabeled"],
          "minRelevanceRank": 5
        }
      ],
      "expectedAnswer": {
        "mustContain": ["supervised", "unsupervised", "label"],
        "mustNotContain": []
      },
      "difficulty": "medium"
    },
